{
  "ActionList": {
    "start": {
      "Arguments": {
        "folder": "tutorial",
        "output1": "sample1.csv",
        "output2": "sample2.csv"
      },
      "InvokeNext": [
        "sum"
      ],
      "FaaSServer": "GH",
      "Type": "R",
      "FunctionName": "create_sample_data"
    },
    "sum": {
      "FunctionName": "compute_sum",
      "FaaSServer": "GH",
      "Type": "R",
      "Arguments": {
        "folder": "tutorial",
        "input2": "sample2.csv",
        "input1": "sample1.csv",
        "output": "sum.csv"
      },
      "InvokeNext": []
    }
  },
  "ComputeServers": {
    "GH": {
      "FaaSType": "GitHubActions",
      "UserName": "renatof",
      "UseSecretStore": true,
      "ActionRepoName": "FaaSr-workflow-nolan",
      "Branch": "main"
    }
  },
  "DataStores": {
    "My_S3_Bucket": {
      "Endpoint": "https://play.min.io",
      "Bucket": "faasr",
      "Region": "us-east-1"
    }
  },
  "ActionContainers": {
    "start": "ghcr.io/faasr/github-actions-r:latest",
    "sum": "ghcr.io/faasr/github-actions-r:latest"
  },
  "FunctionInvoke": "start",
  "DefaultDataStore": "My_S3_Bucket",
  "FunctionGitRepo": {
    "create_sample_data": "FaaSr/FaaSr-tutorial",
    "compute_sum": "FaaSr/FaaSr-tutorial"
  },
  "LoggingDataStore": "My_S3_Bucket",
  "FaaSrLog": "FaaSrLog",
  "InvocationID": "",
  "WorkflowName": "tutorial"
}
