{
    "ComputeServers": {
        "GH": {
            "FaaSType": "GitHubActions",
            "UseSecretStore": true,
            "UserName": "Ashish-Ramrakhiani",
            "ActionRepoName": "FaaSr-workflow-public",
            "Branch": "main"
        }
    },
    "DataStores": {
        "My_S3_Bucket": {
            "Endpoint": "https://play.min.io",
            "Bucket": "faasr",
            "Region": "us-east-1"
        }
    },
    "VMConfig": {
        "Name": "AWS_VM",
        "Provider": "AWS",
        "InstanceId": "i-09405009da063a647",
        "Region": "us-west-2"
    },
    "ActionList": {
        "rfunc": {
            "FunctionName": "r_log",
            "FaaSServer": "GH",
            "Type": "R",
            "RequiresVM": false,
            "Arguments": {
                "message": "Running on GitHub-hosted serverless runner"
            },
            "InvokeNext": [
                "rfunc2"
            ]
        },
        "rfunc2": {
            "FunctionName": "r_log",
            "FaaSServer": "GH",
            "Type": "R",
            "RequiresVM": true,
            "Arguments": {
                "message": "Running on self-hosted EC2 VM"
            },
            "InvokeNext": [
                "faasr-vm-stop"
            ]
        },
        "faasr-vm-start": {
            "FunctionName": "vm_start",
            "FaaSServer": "GH",
            "Type": "Python",
            "RequiresVM": false,
            "InvokeNext": [
                "rfunc"
            ],
            "_faasr_builtin": true
        },
        "faasr-vm-stop": {
            "FunctionName": "vm_stop",
            "FaaSServer": "GH",
            "Type": "Python",
            "RequiresVM": false,
            "InvokeNext": [],
            "_faasr_builtin": true
        }
    },
    "ActionContainers": {
        "rfunc": "ghcr.io/ashish-ramrakhiani/github-actions-r:dev2",
        "rfunc2": "ghcr.io/ashish-ramrakhiani/github-actions-r:dev2",
        "faasr-vm-start": "ghcr.io/ashish-ramrakhiani/github-actions-r:dev2",
        "faasr-vm-stop": "ghcr.io/ashish-ramrakhiani/github-actions-r:dev2"
    },
    "FunctionCRANPackage": {
        "r_log": [
            "gapminder"
        ]
    },
    "FunctionGitRepo": {
        "r_log": "nolcut/test-py"
    },
    "FunctionInvoke": "faasr-vm-start",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_S3_Bucket",
    "DefaultDataStore": "My_S3_Bucket",
    "InvocationID": "1234",
    "InvocationIDFromDate": "%Y%m%d%H%M",
    "WorkflowName": "test-vm-workflow"
}
